
<table>
  <tr>
    <th>*</th>
    <% @group_members.each_with_index do |i,n| %>
      <th>
        <% @x << User.where(id: i.user_id).first.name %>
        <%= @x[n] %>
      </th>
    <% end %>
  </tr>
<% @matrix.each_with_index do |inner,index| %>
  <tr>
    
    <td><%= @x[index] %></td>
    <% inner.each do |elem| %>
      <td><%= elem %></td>
      <% end %>
  </tr>

  <%end%>
</table>